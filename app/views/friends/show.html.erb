<div class="card" >
  <div class="card-body">
    <h1 class="card-title"><%= @friend.first_name %></h5>
    <h2 class="card-subtitle mb-2 text-muted"><%= @friend.last_name %></h6>
    <p class="card-text">
      <%= @friend.email %>
      <%= @friend.phone %>
      <%= @friend.twitter %>
    </p>
    <%= link_to 'Edit', edit_friend_path(@friend), class:"btn btn-secondary" %> 
    <%= link_to 'Back', friends_path, class:"btn btn-secondary btn-dark" %>
  </div>
</div>
